{
  "name": "@cegid/vsb-cds-react",
  "version": "1.19.10",
  "description": "BAAM! Design System by Cegid - React component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.es.js",
      "require": "./dist/components/index.cjs.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.es.js",
      "require": "./dist/components/*/index.cjs.js"
    },
    "./fonts.css": "./dist/fonts/fonts.css",
    "./icons.css": "./dist/icons/hugeicons-font.css"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/components/**/*.js",
    "dist/components/**/*.d.ts",
    "dist/theme/**/*.d.ts",
    "dist/fonts/**/*",
    "dist/icons/**/*",
    "dist/storycomponents/**/*.d.ts"
  ],
  "sideEffects": [
    "**/*.css",
    "**/fonts.css",
    "**/icons.css",
    "**/hugeicons-font.css",
    "dist/styles/**/*.css",
    "dist/icons/**/*.css"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run clean && tsc && vite build && npm run build:optimize",
    "build:dev": "npm run clean && vite build && npm run build:optimize",
    "build:optimize": "node scripts/post-build-cleanup.js",
    "clean": "rimraf dist",
    "storybook": "storybook dev -p 6006",
    "extract-icons": "node scripts/extract-icons.js",
    "build-storybook": "npm run extract-icons && storybook build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "generate-release-notes": "node scripts/generate-release-notes.js",
    "update-release-notes": "node scripts/update-release-notes-story.js",
    "release-notes": "npm run generate-release-notes && npm run update-release-notes",
    "prepare": "husky"
  },
  "keywords": [
    "design-system",
    "react",
    "ui",
    "components"
  ],
  "author": "Cegid",
  "license": "ISC",
  "peerDependencies": {
    "@cegid/cds-react": "^3.26.1",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@cegid/vsb-cds-tokens": "1.0.7",
    "@mui/x-date-pickers": "8.9.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.13",
    "notistack": "2.0.5",
    "react-chartjs-2": "^5.3.0"
  },
  "devDependencies": {
    "@cegid/cds-react": "^3.26.1",
    "@chromatic-com/storybook": "^4.0.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-links": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@types/node": "^22.15.19",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "date-fns": "^4.1.0",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.16",
    "typescript": "^5.0.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.2.4"
  }
}
