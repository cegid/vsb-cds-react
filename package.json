{
  "name": "@cegid/vsb-cds-react",
  "version": "1.4.1",
  "description": "The BIM! by Cegid theme for the Cegid Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.es.js",
      "require": "./dist/components/index.cjs.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.es.js",
      "require": "./dist/components/*/index.cjs.js",
      "types": "./dist/components/*/index.d.ts"
    },
    "./theme": {
      "import": "./dist/theme/index.es.js",
      "require": "./dist/theme/index.cjs.js",
      "types": "./dist/theme/index.d.ts"
    },
    "./theme/*": {
      "import": "./dist/theme/*.es.js",
      "require": "./dist/theme/*.cjs.js",
      "types": "./dist/theme/*.d.ts"
    },
    "./styles/*": "./dist/styles/*",
    "./fonts/*": "./dist/fonts/*",
    "./icons/*": "./dist/icons/*",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/**/*"
  ],
  "sideEffects": [
    "**/*.css",
    "**/fonts.css",
    "**/icons.css",
    "**/hugeicons-font.css",
    "dist/styles/**/*.css",
    "dist/icons/**/*.css"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsc && NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "build:dev": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "design-system",
    "react",
    "ui",
    "components"
  ],
  "author": "Cegid",
  "license": "ISC",
  "peerDependencies": {
    "@cegid/cds-react": "^3.26.1",
    "@cegid/icons-react": "^3.2.3",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cegid/cds-react": "^3.26.1",
    "@cegid/icons-react": "^3.3.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/node": "^22.15.19",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "date-fns": "^4.1.0",
    "glob": "^11.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.12",
    "typescript": "^5.0.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3"
  }
}
